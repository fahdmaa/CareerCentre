{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/",
      "dest": "/server.js"
    },
    {
      "src": "/about(.html)?",
      "dest": "/server.js"
    },
    {
      "src": "/jobs(.html)?",
      "dest": "/server.js"
    },
    {
      "src": "/events(.html)?",
      "dest": "/server.js"
    },
    {
      "src": "/ambassadors(.html)?",
      "dest": "/server.js"
    },
    {
      "src": "/admin-login(.html)?",
      "dest": "/server.js"
    },
    {
      "src": "/admin-dashboard(.html)?",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}