{
  "functions": {
    "server.js": {
      "runtime": "@vercel/node@2.15.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/",
      "destination": "/server.js"
    },
    {
      "source": "/about",
      "destination": "/server.js"
    },
    {
      "source": "/jobs", 
      "destination": "/server.js"
    },
    {
      "source": "/events",
      "destination": "/server.js"
    },
    {
      "source": "/ambassadors",
      "destination": "/server.js"
    },
    {
      "source": "/admin-login",
      "destination": "/server.js"
    },
    {
      "source": "/admin-dashboard",
      "destination": "/server.js"
    }
  ]
}